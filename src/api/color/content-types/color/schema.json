{
  "kind": "collectionType",
  "collectionName": "colors",
  "info": {
    "singularName": "color",
    "pluralName": "colors",
    "displayName": "Color",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "colors"
    },
    "hex": {
      "type": "string",
      "regex": "^[0-9A-Fa-f]{6}$"
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "color"
    }
  }
}
