{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "phoneNumber": {
      "type": "string",
      "unique": false
    },
    "shippingAddress": {
      "type": "text",
      "required": false
    },
    "taxId": {
      "type": "string"
    },
    "birthdate": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "accountStatus": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    }
  }
}
