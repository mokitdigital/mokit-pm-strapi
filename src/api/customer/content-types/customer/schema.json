{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "phoneNumber": {
      "type": "string",
      "unique": false
    },
    "birthdate": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "accountStatus": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    },
    "receivePromotions": {
      "type": "boolean"
    },
    "shareData": {
      "type": "boolean"
    },
    "photo": {
      "type": "string"
    },
    "password": {
      "type": "password"
    },
    "addressMain": {
      "type": "json"
    },
    "addressSecondary": {
      "type": "json"
    },
    "seller": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::seller.seller",
      "inversedBy": "customers"
    },
    "coupons": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::coupon.coupon",
      "mappedBy": "customers"
    }
  }
}
