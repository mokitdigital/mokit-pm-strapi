{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "payment"
    },
    "transactionId": {
      "type": "string",
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "cancelled",
        "in process"
      ],
      "default": "in process"
    }
  }
}
